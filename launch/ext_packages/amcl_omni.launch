<launch>
  <node pkg="amcl" type="amcl" name="amcl">
    <!-- false for kalman filter -->
    <param name="tf_broadcast" value="false" />

    <param name="odom_model_type" value="omni-corrected"/>

    <param name="transform_tolerance" value="0.2" />
    <param name="gui_publish_rate" value="10.0"/>
    <param name="save_pose_rate" value="0.5"/>

    <param name="laser_max_beams" value="30"/>
    <param name="min_particles" value="500"/>
    <param name="max_particles" value="5000"/>
    <!-- odom omni -->
    <param name="kld_err" value="0.05"/>
    <param name="kld_z" value="0.99"/>
    <param name="odom_alpha1" value="0.005"/>
    <param name="odom_alpha2" value="0.005"/>
    <param name="odom_alpha3" value="0.01"/>
    <param name="odom_alpha4" value="0.005"/>
    <param name="odom_alpha5" value="0.003"/>
    <!-- Tuning guide prev_val: 0.5 0.2 0.5 2.0 -->
    <param name="laser_z_hit" value="0.9"/>
    <param name="laser_sigma_hit" value="0.1"/>
    <param name="laser_z_rand" value="0.5"/>
    <param name="laser_likelihood_max_dist" value="4.0"/>

    <param name="laser_z_short" value="0.05"/>
    <param name="laser_z_max" value="0.05"/>
    <param name="laser_lambda_short" value="0.1"/>
    <param name="laser_lambda_short" value="0.1"/>
    <param name="laser_model_type" value="likelihood_field"/>
    

    <!-- make amcl update faster-->
    <param name="update_min_d" value="0.1"/>
    <param name="update_min_a" value="0.25"/>

    <param name="odom_frame_id" value="odom"/>
    <param name="base_frame_id" value="base_footprint"/>

    <param name="initial_pose_x" value="0.0"/>
    <param name="initial_pose_y" value="0.0"/>
    <param name="initial_pose_a" value="0.0"/>

    <param name="initial_cov_xx" value="0.1"/>
    <param name="initial_cov_yy" value="0.1"/>

    <param name="resample_interval" value="1"/>
    <param name="transform_tolerance" value="0.1"/>
    <param name="recovery_alpha_slow" value="0.0"/>
    <param name="recovery_alpha_fast" value="0.0"/>
  </node>
</launch>
